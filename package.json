{
  "name": "mojodns",
  "version": "0.0.7",
  "description": "JavaScript client for the MojoDNS API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "prepublishOnly": "jest",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackFenix2/mojodns-js.git"
  },
  "keywords": [
    "mojodns",
    "dns",
    "api",
    "client"
  ],
  "author": "Ernie Francis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BlackFenix2/mojodns-js/issues"
  },
  "homepage": "https://github.com/BlackFenix2/mojodns-js#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ]
}
